<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type>
    <display-name>FieldSet</display-name>
    <super-type>structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <is-built-in>false</is-built-in>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="documentFieldSet">
            <label>Document</label>
            <items>
                <input name="heading" type="TextLine">
                    <label>Heading</label>
                    <immutable>false</immutable>
                    <indexed>false</indexed>
                    <occurrences minimum="0" maximum="1"/>
                </input>
                <input name="text" type="HtmlArea">
                    <label>Text</label>
                    <immutable>false</immutable>
                    <indexed>false</indexed>
                    <occurrences minimum="0" maximum="1"/>
                    <help-text>HTML Area</help-text>
                </input>
            </items>
        </field-set>
    </form>
</type>
