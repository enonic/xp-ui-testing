<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<type>
    <display-name>All the Input Types</display-name>
    <super-type>structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <is-built-in>false</is-built-in>
    <allow-child-content>true</allow-child-content>
    <form>
        <input name="myColor" type="Color">
            <label>My Color</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myDate" type="Date">
            <label>My Date</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myDecimalNumber" type="DecimalNumber">
            <label>My DecimalNumber</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myGeoLocation" type="GeoLocation">
            <label>My GeoLocation</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myHtmlArea" type="HtmlArea">
            <label>My HtmlArea</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myMoney" type="Money">
            <label>Money</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myPhone" type="Phone">
            <label>Phone</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myComboBox" type="ComboBox">
            <label>ComboBox</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
            <config>
                <options>
                    <option>
                        <label>myOption 1</label>
                        <value>o1</value>
                    </option>
                    <option>
                        <label>myOption 2</label>
                        <value>o2</value>
                    </option>
                </options>
            </config>
        </input>
        <input name="mySingleSelector" type="SingleSelector">
            <label>My SingleSelector</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
            <config>
                <options>
                    <option>
                        <label>myOption 1</label>
                        <value>o1</value>
                    </option>
                    <option>
                        <label>myOption 2</label>
                        <value>o2</value>
                    </option>
                </options>
                <selector-type>DROPDOWN</selector-type>
            </config>
        </input>
        <input name="myTags" type="Tags">
            <label>My Tags</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myTextLine" type="TextLine">
            <label>My TextLine</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myTextArea" type="TextArea">
            <label>My TextArea</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myWholeNumber" type="WholeNumber">
            <label>My WholeNumber</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myXml" type="Xml">
            <label>My Xml</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
        </input>
        <input name="myRelationship" type="Relationship">
            <label>My Relationship</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
            <config>
                <relationship-type>like</relationship-type>
            </config>
        </input>
        <input name="myImageSelector" type="ImageSelector">
            <label>My ImageSelector</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="0"/>
            <config>
                <relationship-type>default</relationship-type>
            </config>
        </input>
    </form>
</type>
